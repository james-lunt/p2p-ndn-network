#Run source .tmux/ndn-network to start this
#tmux attach to control
#tmux kill-server to shut down

tmux new-session -d -s NDN
tmux new-window -n d1
tmux new-window -d -n d2
tmux send-keys -t NDN:bash "echo Base Control"
tmux send-keys -t NDN:d1 "python node.py --name /divers/diver1/light" 
tmux send-keys -t NDN:d2 "python node.py --name /divers/diver2" C-m